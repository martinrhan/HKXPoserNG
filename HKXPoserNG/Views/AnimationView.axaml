<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="100"
             x:Class="HKXPoserNG.Views.AnimationView">
  <DockPanel>
	<TextBlock Text="{Binding CurrentFrame}"></TextBlock>
	<Slider Value="{Binding CurrentFrame}" Maximum="{Binding FrameCountMinusOne}" Name="slider"></Slider>
  </DockPanel>
</UserControl>
